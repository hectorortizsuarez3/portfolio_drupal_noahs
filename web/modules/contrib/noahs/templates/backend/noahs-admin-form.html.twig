<div id="top-panel">
    <button id="btn-desktop" class="btn-top-panel active" data-bs-toggle="tooltip" data-bs-placement="bottom" data-query="desktop" title="Desktop">
      <i class="las la-desktop"></i> 
    </button>
    <button id="btn-tablet" class="btn-top-panel" data-bs-toggle="tooltip" data-bs-placement="bottom" data-query="tablet" title="Tablet">
      <i class="las la-tablet-alt"></i>
    </button>
    <button id="btn-mobile" class="btn-top-panel" data-bs-toggle="tooltip" data-bs-placement="bottom" data-query="mobile" title="Mobile">
      <i class="las la-mobile-alt"></i>
    </button>
    <button class="btn btn-outline-dark btn-small" data-bs-toggle="collapse" data-bs-toggle="collapse" data-bs-target="#showNoahsStructure">
      <span data-bs-toggle="tooltip" data-bs-placement="bottom" title="{{ 'Navigator' | t}}">
        <i class="las la-layer-group"></i>
      </span>
    </button>
    <div class="btn-group ms-4">
      <button id="backButton" class="btn btn-outline-dark btn-small" data-bs-toggle="tooltip" data-bs-placement="bottom" title="{{ 'Prev' | t}}">
        <i class="las la-reply"></i>
      </button>
      <button id="forwardButton" class="btn btn-outline-dark btn-small" data-bs-toggle="tooltip" data-bs-placement="bottom" title="{{ 'Next' | t}}">
        <i class="las la-share"></i>
      </button>
    </div>
    <div id="noahs-window-scale" class="input-group ms-2">
      <input type="number" value="100" max="100" step="5" min="20" class="form-control" />
      <span class="input-group-text">%</span>
    </div>
    <div id="noahs-window-size" class="input-group">
      <input type="number" value="" step="10" min="320" class="form-control" />
      <span class="input-group-text">px</span>
    </div>
</div>
<div id="showNoahsStructure" class="collapse draggable">
    <div class="header">
      <div>{{'Navigator' | t}}</div>
      <button class="btn btn-sm me-2">
        <i class="las la-times"></i>
      </button>
    </div>
    <div class="items"></div>
</div>
<div class="noahs_page_builder_preview-wrapper">
    <div id="pageMessages"></div>
    <div class="noahs_page_builder_preview-content">
        <iframe
            src="{{iframe_url}}"
            id="noahs_page_builder_preview" name="noahs_page_builder_preview"></iframe>
        <div class="l ui-resizable-handle ui-resizable-w resizable-wrapper" id="w"></div>
        <div class="r ui-resizable-handle ui-resizable-e resizable-wrapper" id="e"></div>
    </div>
</div>
<form id="noahs_page_builder_form_builder" class="noahs_page_builder_form_builder">
    <div id="widgets_to_remove" data-remove-widgets=""></div>
    <button
        type="button"
        class="btn btn-link btn-sm area_tooltip btn-admin btn-outline-dark"
        data-bs-toggle="modal"
        data-bs-target="#staticBackdrop"
        title="{{ 'Page settings' | t}}">
        <i class="las la-tools"></i>
    </button>
    <a href="{{url}}" class="btn btn-outline-info btn-sm ms-2 btn-admin" target="_blank">{{'View page' | t}}</a>
    <a href="/admin/content" class="btn btn-outline-primary btn-sm ms-2 btn-admin" target="_blank">{{'Content page' | t}}</a>
    {% if noahs_pro %}
      <a
      href="/noahs-admin/revisions/{{noahs_id}}/{{entity_type}}/{{langcode}}"
      data-dialog-options="{&quot;width&quot;:400}" 
      data-dialog-type="modal" 
      type="button"
      class="btn btn-outline-info btn-sm ms-2 btn-admin f-admin use-ajax"
      title="{{ 'Revisions' | t}}">
      <i class="las la-bars"></i>
      {{ 'Revisions' | t}}
      </a>
    {% endif %}
    {% if noahs_ai %}
        <div class="dropdown">
            <button class="btn btn-outline-dark ms-2 dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
                {{ 'Noahs AI' | t }}
            </button>
            <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
                <li><button type="button" class="btn btn-sm ms-2 noahs-ai-translate-page btn-outline-dark btn-admin dropdown-item">{{ 'Translate' | t }}</button></li>
                <li><button id="noahs-ai-generate-btn" type="button" class="btn btn-admin btn-outline-primary ms-2 dropdown-item"> {{ 'Design with Noahs AI' | t }}</button></li>
                <li><button id="noahs-ai-import-run" type="button" class="btn btn-admin btn-outline-primary ms-2 dropdown-item">{{ 'Import Design from Image' | t }}</button></li>
            </ul>
        </div>

    {% endif %}
    <button type="submit" class="btn btn-success btn-sm ms-2 btn-admin px-4">{{'Save' | t}}</button>
</form>

<!-- Modal Page Settings-->
<div
    class="modal fade"
    id="staticBackdrop">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">{{'Page Settings' | t}}</h5>
                <button
                    type="button"
                    class="btn-close"
                    data-bs-dismiss="modal"
                    aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form id="noahs_page_builder_sttings_page_form" class="noahs-buider-form">
                  {{render_form.0 | raw}}
                  <div class="btn-group d-flex justify-content-end">
                      <button type="submit" id="savenoahs_page_builderPageSettings" class="btn btn-success btn-admin">{{ 'Save settings' |t }}</button>
                      <a type="button" class="btn btn-outline-danger btn-admin" data-bs-dismiss="modal">{{ 'Close' |t }}</a>
                  </div>
              </form>
            </div>

        </div>
    </div>
</div>

<!-- Modal Add Column -->

<div class="modal fade"
    id="noahsAddColumn">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{{'Add Column' | t}}</h5>
                <button
                    type="button"
                    class="btn-close"
                    data-bs-dismiss="modal"
                    aria-label="Close"></button>
            </div>
            <div class="modal-body">
                {% set classes = {
                    'wd-10': '10%',
                    'wd-20': '20%',
                    'wd-30': '30%',
                    'wd-40': '40%',
                    'wd-50': '50%',
                    'wd-60': '60%',
                    'wd-70': '70%',
                    'wd-80': '80%',
                    'wd-90': '90%',
                    'wd-100': '100%'
                } %}

                <div class="noahs_page_builder-column-modal">
                    <ul class="row">
                        {% for key, value in classes %}
                            <li class="column-box {{ key }} area_tooltip" title="{{ value }}" data-column="{{ value }}"></li>
                        {% endfor %}
                        <li class="column-box area_tooltip columns_structure" title="{{ 'Two colums' | t }}" data-column="two_columns"><span></span><span></span></li>
                        <li class="column-box area_tooltip columns_structure" title="{{ 'Three colums' | t }}" data-column="three_columns"><span></span><span></span><span></span></li>
                        <li class="column-box area_tooltip columns_structure" title="{{ 'Four colums' | t }}" data-column="four_columns"><span></span><span></span><span></span><span></span></li>
                        <li class="column-box area_tooltip columns_60_40" title="{{ 'Two colums 60/40' | t }}" data-column="columns_60_40"><span></span><span></span><span></span><span></span></li>
                        <li class="column-box area_tooltip columns_40_60" title="{{ 'Two colums 40/60' | t }}" data-column="columns_40_60"><span></span><span></span><span></span><span></span></li>
                        <li class="column-box area_tooltip columns_70_30" title="{{ 'Two colums 70/30' | t }}" data-column="columns_70_30"><span></span><span></span><span></span><span></span></li>
                        <li class="column-box area_tooltip columns_30_70" title="{{ 'Two colums 30/70' | t }}" data-column="columns_30_70"><span></span><span></span><span></span><span></span></li>
                    </ul>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-danger btn-admin" data-bs-dismiss="modal">{{ 'Cancel' |t }}</button>
            </div>
        </div>
    </div>
</div>


<div class="modal fade" id="addElementWidgetModal">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{{'Available widgets' | t}}</h5>
                <div class="input-group noahs-search-widget">
                  <input type="text" id="searchWidget" class="form-control">
                  <span class="input-group-text"><i class="las la-search"></i></span>
                </div>
                <button
                    type="button"
                    class="btn-close"
                    data-bs-dismiss="modal">
                </button>
            </div>
            <nav>
                <div class="nav nav-tabs nav nav-tabs justify-content-center mt-3" id="nav-tab" role="tablist">
                    <button type="button" class="nav-link active" id="nav-widgets-tab" data-bs-toggle="tab" data-bs-target="#nav-widgets" type="button" role="tab" aria-controls="nav-widgets" aria-selected="true">{{ 'Widgets' | t }}</button>
                    <button type="button" class="nav-link" id="nav-globales-tab" data-bs-toggle="tab" data-bs-target="#nav-globales" type="button" role="tab" aria-controls="nav-globales" aria-selected="false">{{ 'Global Widgets' | t }}</button>
                    <button type="button" class="nav-link" id="nav-defaults-tab" data-bs-toggle="tab" data-bs-target="#nav-defaults" type="button" role="tab" aria-controls="nav-defaults" aria-selected="false">{{ 'Defauls Widgets' | t }}</button>
                </div>
            </nav>
            <div class="modal-body">
                <div class="tab-content" id="nav-tabContentwidgets">
                    <div class="tab-pane fade show active" id="nav-widgets" role="tabpanel" aria-labelledby="nav-widgets-tab">
